{
  "name": "hr-command-center",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:a11y": "jest __tests__/accessibility",
    "test:e2e": "playwright test",
    "test:e2e:a11y": "playwright test e2e/accessibility.spec.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:a11y && npm run test:e2e:a11y",
    "validate": "npm run type-check && npm run lint && npm run test",
    "clean": "rm -rf .next out node_modules/.cache",
    "migrate:json-to-sqlite": "tsx ../scripts/migrate-json-to-sqlite.ts",
    "db:stats": "tsx ../scripts/db-stats.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@google-cloud/aiplatform": "^5.12.0",
    "@google-cloud/dlp": "^6.5.0",
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "axios": "^1.7.0",
    "better-sqlite3": "^12.4.1",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.6",
    "drizzle-orm": "^0.44.7",
    "framer-motion": "^12.23.24",
    "jose": "^6.1.0",
    "lucide-react": "^0.552.0",
    "next": "^14.2.0",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "openai": "^6.8.1",
    "opossum": "^9.0.0",
    "p-retry": "^7.1.0",
    "papaparse": "^5.5.3",
    "react": "^18.3.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.3.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.66.0",
    "react-markdown": "^10.1.0",
    "recharts": "^2.15.4",
    "remark-gfm": "^4.0.1",
    "simple-statistics": "^7.8.8",
    "sonner": "^2.0.7",
    "string-similarity": "^4.0.4",
    "swr": "^2.3.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.1/xlsx-0.20.1.tgz",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@axe-core/react": "^4.11.0",
    "@next/bundle-analyzer": "^16.0.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.0.0",
    "@types/opossum": "^8.1.9",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/string-similarity": "^4.0.2",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.2.0",
    "jest": "^30.2.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.4.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.0",
    "ts-prune": "^0.10.3",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0"
  }
