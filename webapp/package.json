{
  "name": "hr-command-center",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "biome check --error-on-warnings .",
    "lint:fix": "biome check --write --unsafe .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "type-check": "tsc --noEmit",
    "test": "NODE_ENV=test vitest run",
    "test:watch": "NODE_ENV=test vitest",
    "test:coverage": "NODE_ENV=test vitest run --coverage",
    "test:a11y": "NODE_ENV=test vitest run __tests__/accessibility",
    "test:ui": "NODE_ENV=test vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:a11y": "playwright test e2e/accessibility.spec.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:a11y && npm run test:e2e:a11y",
    "validate": "npm run type-check && npm run format:check && npm run lint && npm run test",
    "clean": "rm -rf .next out node_modules/.cache",
    "migrate:json-to-sqlite": "tsx ../scripts/migrate-json-to-sqlite.ts",
    "db:stats": "tsx ../scripts/db-stats.ts",
    "db:apply-indexes": "tsx scripts/apply-performance-indexes.ts",
    "db:test-performance": "tsx scripts/test-performance-improvements.ts",
    "seed:enps": "tsx ../scripts/seed-enps-data.ts",
    "test:lighthouse": "lhci autorun",
    "test:performance": "npm run test:lighthouse && npm run db:test-performance",
    "prepare": "husky"
  },
  "dependencies": {
